    "@types/node": "^17.0.23",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/semver": "^7.3.9",
    "@types/styled-components": "^5.1.25",
    "@types/tar-fs": "^2.0.1",
    "babel-plugin-styled-components": "^2.0.7",
    "copy-webpack-plugin": "^6.4.1",
    "electron": "^18.0.3",
